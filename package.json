{
  "name": "poker-utils",
  "version": "13.0.4",
  "description": "Fast poker ranges, evaluation, equity calculation",
  "author": "Conrad Kay",
  "repository": "https://github.com/conradbkay/poker-utils",
  "license": "MIT",
  "keywords": [],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "pkgroll",
    "bench": "tsx --expose-gc src/benchmarks/benchmark.ts",
    "bench:readme": "tsx --expose-gc src/benchmarks/benchmark.ts -md",
    "test": "tsx --test ./src/tests/*.ts",
    "test:watch": "tsx watch --test ./src/tests/*.ts"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.10",
    "@types/node": "^20.14.9",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "mitata": "^1.0.34",
    "npm-run-all": "^4.1.5",
    "phe": "^0.6.0",
    "pkgroll": "^2.14.3",
    "prettier": "^3.3.3",
    "semver": "^7.7.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
